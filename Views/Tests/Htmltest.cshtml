@{
    Layout = "_TestLayout.cshtml";
}



<script>

    module('html5 features', {
        setup: function () {
            var fixture = $('#qunit-fixture');
            fixture.append('<input type="search" id="txtsearch" name="txtsearch"/>');
            console.log('html5 feature test setup');
        }
    });

    test('modernizr feature suport', function(){
        
        ok(Modernizr.sessionstorage, 'session storage supported');
        ok(Modernizr.inputtypes.date, 'html 5 date supported');
        ok(Modernizr.inputtypes.email, 'html 5 email  supported');
        ok(Modernizr.inputtypes.search, 'html 5 search  supported');
        ok(Modernizr.geolocation, 'html5 geolcation supported');
        // test loading for non-supporting browser
        //Modernizr.load({
        //    test: Modernizr.canvas,
        //    nope: 'http://flashcanvas.net/bin/flashcanvas.js'
        //});
    });
</script>
